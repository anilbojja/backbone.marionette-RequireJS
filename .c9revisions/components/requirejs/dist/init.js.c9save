{"ts":1354076541628,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"$(document).ready(function($) {\n\n    // set panel min-height to full browser height\n    $(window).bind(\"load resize\", function(){\n        var h = $(window).height();\n        $(\"#wrapper, #grid, #navBg\").css({ \"min-height\" : (h) });\n        $(\"#content\").css({ \"min-height\" : (h-168) });\n    });\n\n    // toggle grid\n\n    $(\"#grid\").hide();\n\n    $(document.documentElement).keyup(function (event) {\n        if (event.keyCode == 71) {\n            $(\"#grid\").fadeToggle(100);\n        }\n    });\n\n    // if window is larger than #nav then #nav == fixed, if #nav is larger than window #nav == relative\n    $(window).bind(\"load resize\", function(){\n        var w = $(window).height();\n        var h = $(\"#nav\").outerHeight();\n        $(\"#nav\").css(\"position\",(w < h) ? \"\" : \"fixed\");\n    });\n\n    // done!\n});\n"]],"start1":0,"start2":0,"length1":0,"length2":802}]],"length":802}
